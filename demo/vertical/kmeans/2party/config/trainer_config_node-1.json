[
    {
        "identity": "label_trainer",
        "model_info": {
            "name": "vertical_kmeans"
        },
        "input": {
            "trainset": [
                {
                    "type": "csv",
                    "path": "./dataset/breast_cancer_wisconsin_vertical/2party",
                    "name": "breast_cancer_wisconsin_vertical_labeled_train.csv",
                    "has_label": true,
                    "has_id": true
                }
            ]
        },
        "output": {
            "path": "/opt/checkpoints/[JOB_ID]/[NODE_ID]",
            "model": {
                "name": "vertical_kmeans_[STAGE_ID].pkl"
            },
            "result": {
                "name": "cluster_result_[STAGE_ID].csv"
            },
            "summary": {
                "name": "cluster_summary_[STAGE_ID].csv"
            }
        },
        "train_info": {
            "train_params": {
                "init": "random",
                "encryption": {
                    "otp": {
                        "key_bitlength": 128,
                        "data_type": "torch.Tensor",
                        "key_exchange": {
                            "key_bitlength": 3072,
                            "optimized": true
                        },
                        "csprng": {
                            "name": "hmac_drbg",
                            "method": "sha512"
                        }
                    }
                },
                "k": 5,
                "max_iter": 99,
                "tol": 1e-3,
                "random_seed": 50
            }
        }
    }
]