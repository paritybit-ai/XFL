[
    {
        "identity": "label_trainer",
        "model_info": {
            "name": "horizontal_gcn_mol",
            "config": {
                "input_dim": 74,
                "num_gnn_layers": 1,
                "gnn_hidden_feats": 256,
                "activation": "relu",
                "dropout": 0.001,
                "predictor_hidden_feats": 512,
                "batchnorm": false,
                "residual": false
            }
        },
        "input": {
            "trainset": [
                {
                    "type": "csv",
                    "path": "./dataset/hiv/2party",
                    "name": "hiv_train_2.csv",
                    "has_label": true,
                    "has_id": false
                }
            ]
        },
        "output": {
            "metrics": {
                "type": "csv",
                "path": "/opt/checkpoints/[JOB_ID]/[NODE_ID]",
                "header": true
            },
            "evaluation": {
                "type": "csv",
                "path": "/opt/checkpoints/[JOB_ID]/[NODE_ID]",
                "header": true
            }
        },
        "train_info": {
            "device": "cpu",
            "interaction_params": {
                "save_frequency": 2,
                "save_probabilities": true,
                "save_probabilities_bins_number": 10,
                "write_training_prediction": true,
                "write_validation_prediction": false,
                "echo_training_metrics": true
            },
            "params": {
                "global_epoch": 50,
                "local_epoch": 1,
                "batch_size": 128,
                "aggregation_config": {
                    "type": "fedavg",
                    "encryption": {
                        "method": "otp",
                        "key_bitlength": 64,
                        "data_type": "torch.Tensor",
                        "key_exchange": {
                            "key_bitlength": 3072,
                            "optimized": true
                        },
                        "csprng": {
                            "name": "hmac_drbg",
                            "method": "sha512"
                        }
                    }
                },
                "optimizer_config": {
                    "Adam": {
                        "lr": 0.0005,
                        "weight_decay": 0.001
                    }
                },
                "lossfunc_config": {
                    "BCEWithLogitsLoss": {}
                },
                "lr_scheduler_config": {
                    "CosineAnnealingLR": {
                        "T_max": 200
                    }
                },
                "metric_config": {
                    "accuracy": {},
                    "roc_auc_score": {}
                },
                "early_stopping": {
                    "key": "acc",
                    "patience": 2,
                    "delta": 0
                },
                "extra_config": {}
            }
        }
    }
]