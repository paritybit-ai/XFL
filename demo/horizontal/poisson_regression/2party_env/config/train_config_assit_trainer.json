[
    {
        "identity": "assist_trainer",
        "model_info": {
            "name": "horizontal_poisson_regression",
            "config": {
                "input_dim": 3,
                "bias": true
            }
        },
        "input": {
            "valset": [
                {
                    "type": "csv",
                    "path": "./dataset/arizona_card_hos",
                    "name": "ari_card_hos_test.csv",
                    "has_label": true,
                    "has_id": false
                }
            ],
            "pretrain_model": {}
        },
        "output": {
            "model": {
                "type": "file",
                "path": "/opt/checkpoints/[JOB_ID]/[NODE_ID]",
                "name": "horizontal_poisson_regression.pt"
            },
            "metrics": {
                "type": "csv",
                "path": "/opt/checkpoints/[JOB_ID]/[NODE_ID]",
                "header": true
            },
            "evaluation": {
                "type": "csv",
                "path": "/opt/checkpoints/[JOB_ID]/[NODE_ID]",
                "header": true
            }
        },
        "train_info": {
            "device": "cpu",
            "interaction_params": {
                "save_frequency": -1,
                "save_probabilities": true,
                "save_probabilities_bins_number": 10,
                "write_training_prediction": true,
                "write_validation_prediction": true,
                "echo_training_metrics": true
            },
            "params": {
                "global_epoch": 30,
                "local_epoch": 1,
                "batch_size": 32,
                "aggregation_config": {
                    "type": "fedavg",
                    "encryption": {
                        "method": "otp",
                        "key_bitlength": 64,
                        "data_type": "torch.Tensor",
                        "key_exchange": {
                            "key_bitlength": 3072,
                            "optimized": true
                        },
                        "csprng": {
                            "name": "hmac_drbg",
                            "method": "sha512"
                        }
                    }
                },
                "lossfunc_config": {
                    "PoissonNLLLoss": {
                        "log_input": false
                    }
                },
                "metric_config": {
                    "mean_poisson_deviance": {}
                },
                "extra_config": {}
            }
        }
    }
]