{
    "identity": "label_trainer",
    "model_info": {
        "name": "vertical_binning_woe_iv"
    },
    "input": {
        "trainset": [
            {
                "type": "csv",
                "path": "../../../../dataset/vertical_breast_with_validation",
                "name": "breast_vertical_guest_train.csv",
                "has_id": true,
                "has_label": true,
                "nan_list": [
                    999,
                    -999
                ]
            }
        ]
    },
    "output": {
        "path": "/opt/checkpoints/[JOB_ID]/[NODE_ID]",
        "result": {
            "name": "woe_iv_result_[STAGE_ID].json"
        },
        "split_points": {
            "name": "binning_split_points_[STAGE_ID].json"
        }
    },
    "train_info": {
        "train_params": {
            "encryption": {
                "paillier": {
                    "key_bit_size": 2048,
                    "precision": 7,
                    "djn_on": true,
                    "parallelize_on": true
                }
            },
            "binning": {
                "method": "equal_frequency",
                "bins": 5
            }
        }
    }
}
