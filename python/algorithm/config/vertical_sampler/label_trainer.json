{
    "identity": "label_trainer",
    "model_info": {
        "name": "vertical_sampler"
    },
    "input": {
        "dataset": [
            {
                "type": "csv",
                "path": "../dataset/vertical_breast_with_validation",
                "name": "breast_vertical_guest_train.csv",
                "has_label": true,
                "has_id": true
            }
        ]
    },
    "output": {
        "path": "/tmp/xfl/checkpoints/[JOB_ID]/[NODE_ID]",
        "sample_id": {
            "name": "temp/sampled_id_[STAGE_ID].json"
        },
        "dataset": {
            "name": "temp/sampled_data_[STAGE_ID].csv"
        }
    },
    "train_info": {
        "train_params": {
            "method": "random",
            "strategy": "downsample",
            "random_seed": 42,
            "fraction": {
                "percentage": 0.1
            },
            "marketing_specified": {
                "threshold_method": "percentage",
                "threshold": 0.1
            }
        }
    }
}